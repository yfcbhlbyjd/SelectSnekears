<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".paperwork.delivery_address.DeliveryAddressFragment">

    <RelativeLayout
        android:id="@+id/top_app_bar"
        android:layout_width="360dp"
        android:layout_height="64dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/top_app_bar"
        />

    <TextView
        android:id="@+id/tv_new_address"
        android:layout_width="122dp"
        android:layout_height="22dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="98dp"
        android:text="@string/some_id"
        android:layout_below="@id/top_app_bar"
        android:textAppearance="@style/some_id"
        android:lineSpacingExtra="-1sp"
        android:gravity="center_vertical"
        />

    <RelativeLayout
        android:id="@+id/frame_359"
        android:layout_width="332dp"
        android:layout_height="46dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="106dp"
        android:background="@drawable/frame_359">

        <TextView
            android:id="@+id/tv_country"
            android:layout_width="132dp"
            android:layout_height="14dp"
            android:text="@string/country"
            android:layout_alignBottom="@id/tv_name_country"
            android:textAppearance="@style/small_size"
            android:gravity="center_vertical"
            />
        <TextView
            android:id="@+id/tv_name_country"
            android:layout_width="220dp"
            android:layout_height="20dp"
            android:text="@string/name_country"
            android:textAppearance="@style/average_size"
            android:gravity="center_vertical"
            />
        <View
            android:id="@+id/vector"
            android:layout_width="15.84dp"
            android:layout_height="7.1dp"
            android:background="@drawable/vector"
            />
</RelativeLayout>

    <RelativeLayout
        android:id="@+id/frame_360"
        android:layout_width="332dp"
        android:layout_height="45.84dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="157dp"
        android:background="@drawable/frame_360">
        <TextView
        android:id="@+id/tv_city"
        android:layout_width="342dp"
        android:layout_height="20dp"
        android:text="@string/city"
        android:textAppearance="@style/small_size"
        android:gravity="center_vertical"/>
        <View
            android:id="@+id/vector2"
            android:layout_width="15.84dp"
            android:layout_height="7.1dp"
            android:background="@drawable/vector"
            />
        />
    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/frame_351"
        android:layout_width="332dp"
        android:layout_height="46dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="208dp"
        android:background="@drawable/frame_351">
        <TextView
            android:id="@+id/tv_street"
            android:layout_width="312dp"
            android:layout_height="20dp"
            android:text="@string/some_id"
            android:textAppearance="@style/some_id"
            android:gravity="center_vertical"
        />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/frame_352"
        android:layout_width="332dp"
        android:layout_height="46dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="259dp"
        android:background="@drawable/frame_352">
        <TextView
            android:id="@+id/tv_home"
            android:layout_width="312dp"
            android:layout_height="20dp"
            android:text="@string/home"
            android:textAppearance="@style/size"
            android:gravity="center_vertical"/>
    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/frame_353"
        android:layout_width="332dp"
        android:layout_height="46dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="310dp"
        android:background="@drawable/frame_353"

        >
        <TextView
            android:id="@+id/tv_index"
            android:layout_width="312dp"
            android:layout_height="20dp"
            android:text="@string/index"
            android:textAppearance="@style/size"
            android:gravity="center_vertical"
            />
    </RelativeLayout>

    <TextView
        android:id="@+id/some_id2"
        android:layout_width="183dp"
        android:layout_height="22dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="371dp"
        android:text="@string/some_id2"
        android:textAppearance="@style/some_id"
        android:lineSpacingExtra="-1sp"
        android:gravity="center_vertical"
        />

    <RelativeLayout
        android:id="@+id/frame_354"
        android:layout_width="332dp"
        android:layout_height="46dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="403dp"
        android:background="@drawable/frame_354">
        <TextView
            android:id="@+id/tv_name_user"
            android:layout_width="312dp"
            android:layout_height="20dp"
            android:text="@string/some_id"
            android:textAppearance="@style/some_id"
            android:gravity="center_vertical"
            />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/frame_355"
        android:layout_width="332dp"
        android:layout_height="46dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="454dp"
        android:background="@drawable/frame_355">
        <TextView
            android:id="@+id/tv_surname"
            android:layout_width="312dp"
            android:layout_height="20dp"
            android:text="@string/surname"
            android:textAppearance="@style/size"
            android:gravity="center_vertical"
        />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/frame_357"
        android:layout_width="332dp"
        android:layout_height="46dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="505dp"
        android:background="@drawable/frame_357"
        >
        <TextView
            android:id="@+id/tv_code"
            android:layout_width="129dp"
            android:layout_height="14dp"
            android:text="@string/code"
            android:textAppearance="@style/small_size"
            android:gravity="center_vertical"
            />
        <TextView
            android:id="@+id/tv_code_country"
            android:layout_width="38dp"
            android:layout_height="20dp"
            android:text="@string/code_country"
            android:textAppearance="@style/average_size"
            android:gravity="center_vertical"
            />
        <TextView
            android:id="@+id/tv_phone_number"
            android:layout_width="254dp"
            android:layout_height="20dp"
            android:text="@string/phone_number"
            android:textAppearance="@style/size"
            android:gravity="center_vertical"
            />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/frame_356"
        android:layout_width="332dp"
        android:layout_height="46dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="556dp"
        android:background="@drawable/frame_356"
        >
        <TextView
            android:id="@+id/tv_email"
            android:layout_width="312dp"
            android:layout_height="20dp"
            android:text="@string/email"
            android:textAppearance="@style/size"
            android:gravity="center_vertical"
            />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/btn_save"
        android:layout_width="332dp"
        android:layout_height="54dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="692dp"
        android:background="@drawable/some_id3"
        >

        <TextView
            android:id="@+id/tv_save"
            android:layout_width="107dp"
            android:layout_height="24dp"
            android:layout_marginTop="15dp"
            android:layout_marginStart="10dp"
            android:gravity="top"
            android:text="@string/save"
            android:textAppearance="@style/text_start_update" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/home"
        android:layout_width="360dp"
        android:layout_height="20dp"
        android:layout_marginTop="756dp"
        android:background="@drawable/home"
        />
</FrameLayout>