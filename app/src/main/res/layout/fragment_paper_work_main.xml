            android:layout_width="133dp"
            android:layout_height="22dp"
            android:text="@string/nike_sneake"
            android:textAppearance="@style/nike_sneake"
            android:lineSpacingExtra="6sp"
            android:gravity="end|center_vertical"
            />
        <TextView
            android:id="@+id/tv_in_cost"
            android:layout_width="133dp"
            android:layout_height="22dp"
            android:text="@string/in_cost"
            android:textAppearance="@style/cost"
            android:lineSpacingExtra="6sp"
            android:gravity="end|center_vertical"
            />
        <TextView
            android:id="@+id/tv_free"
            android:layout_width="133dp"
            android:layout_height="22dp"
            android:text="@string/free"
            android:textAppearance="@style/some_id"
            android:lineSpacingExtra="6sp"
            android:gravity="end|center_vertical"
            />
        <TextView
            android:id="@+id/tv_discount_result"
            android:layout_width="133dp"
            android:layout_height="22dp"
            android:text="@string/discount_result"
            android:textAppearance="@style/some_id"
            android:lineSpacingExtra="6sp"
            android:gravity="end|center_vertical"
            />
        <TextView
            android:id="@+id/tv_result_promo_code_discount"
            android:layout_width="103dp"
            android:layout_height="22dp"
            android:text="@string/result_promo_code_discount"
            android:textAppearance="@style/some_id"
            android:lineSpacingExtra="6sp"
            android:gravity="end|center_vertical"
            />
