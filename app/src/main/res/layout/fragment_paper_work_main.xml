            android:layout_height="17dp"
            android:text="@string/size"
            android:textAppearance="@style/size"
            android:lineSpacingExtra="-2sp"
            android:gravity="center_vertical"
            />
        <View
            android:id="@+id/vector"
            android:layout_width="15.84dp"
            android:layout_height="7.1dp"
            android:background="@drawable/vector"
            />
        <View
            android:id="@+id/line_19"
            android:layout_width="312dp"
            android:layout_height="0dp"
            android:background="@drawable/line_19"
            />

        <TextView
            android:id="@+id/tv_quantity"
            android:layout_width="54dp"
            android:layout_height="17dp"
            android:gravity="center_vertical"
            android:lineSpacingExtra="-2sp"
            android:text="@string/quantity"
            android:textAppearance="@style/some_id" />

        <TextView
            android:id="@+id/tv_minus"
            android:layout_width="6dp"
            android:layout_height="17dp"
            android:text="@string/minus"
            android:textAppearance="@style/quantity"
            android:lineSpacingExtra="-2sp"
            android:gravity="center_vertical"
            />
        <TextView
            android:id="@+id/tv_plus"
            android:layout_width="9dp"
            android:layout_height="17dp"
            android:text="@string/plus"
            android:textAppearance="@style/quantity"
            android:lineSpacingExtra="-2sp"
            android:gravity="center_vertical"
            />
        <TextView
            android:id="@+id/tv_cost"
            android:layout_width="122dp"
            android:layout_height="25dp"
            android:text="@string/cost"
            android:textAppearance="@style/cost"
            android:lineSpacingExtra="2sp"
            android:translationY="-0.78sp"
            android:gravity="end|top"/>

        <TextView
            android:id="@+id/tv_name"
            android:layout_width="138dp"
            android:layout_height="22dp"
            android:text="@string/name"
            android:textAppearance="@style/quantity"
            android:lineSpacingExtra="6sp"
            android:gravity="center_vertical"
            />
        <TextView
            android:id="@+id/tv_price"
            android:layout_width="142dp"
            android:layout_height="22dp"
            android:text="@string/price"
            android:textAppearance="@style/price"
            android:lineSpacingExtra="6sp"
            android:gravity="center_vertical"
            />
        <TextView
            android:id="@+id/tv_delivery"
            android:layout_width="124dp"
            android:layout_height="22dp"
            android:text="@string/delivery"
            android:textAppearance="@style/price"
            android:lineSpacingExtra="6sp"
            android:gravity="center_vertical"
            />
        <TextView
            android:id="@+id/tv_discount"
            android:layout_width="129dp"
            android:layout_height="22dp"
            android:text="@string/discount"
            android:textAppearance="@style/price"
            android:lineSpacingExtra="6sp"
            android:gravity="center_vertical"
            />
