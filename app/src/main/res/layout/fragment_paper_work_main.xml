        android:elevation="4dp">
        <TextView
            android:id="@+id/tv_add_new_address"
            android:layout_width="178dp"
            android:layout_height="22dp"
            android:text="@string/add_new_address"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:textAppearance="@style/some_id"
            android:lineSpacingExtra="2sp"
            android:gravity="center_vertical"/>
    </RelativeLayout>

    <TextView
        android:id="@+id/tv_payment"
        android:layout_width="66dp"
        android:layout_height="22dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="181dp"
        android:text="@string/payment"
        android:textAppearance="@style/some_id"
        android:lineSpacingExtra="-1sp"
        android:gravity="center_vertical"
        />

    <RelativeLayout
        android:id="@+id/frame_308"
        android:layout_width="332dp"
        android:layout_height="50dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="208dp"
        android:background="@drawable/frame_308"
        android:elevation="4dp">
        <TextView
            android:id="@+id/tv_select_payment_method"
            android:layout_width="195dp"
            android:layout_height="22dp"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="@string/select_payment_method"
            android:textAppearance="@style/some_id"
            android:lineSpacingExtra="2sp"
            android:gravity="center_vertical"
            />
    </RelativeLayout>

    <TextView
        android:id="@+id/tv_goods"
        android:layout_width="70dp"
        android:layout_height="22dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="278dp"
        android:text="@string/tv_goods"
        android:textAppearance="@style/some_id"
        android:lineSpacingExtra="-1sp"
        android:gravity="center_vertical"
        />

    <RelativeLayout
        android:id="@+id/frame_442"
        android:layout_width="332dp"
        android:layout_height="373dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="305dp"
        android:background="@drawable/frame_442"
