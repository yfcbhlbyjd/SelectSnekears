<RelativeLayout android:id="@+id/frame_442"
    android:layout_width="332dp"
    android:layout_height="373dp"
    android:layout_marginStart="14dp"
    android:layout_marginTop="305dp"
    android:background="@drawable/frame_442"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <TextView
        android:id="@+id/nike_sneake"
        android:layout_width="192dp"
        android:layout_height="21dp"
        android:layout_alignParentStart="true"
        android:text="@string/nike_sneake"
        android:gravity="top" />

    <View
        android:id="@+id/container"
        android:layout_width="144dp"
        android:layout_height="116dp"
        android:layout_below="@+id/nike_sneake"
        android:layout_alignParentStart="true"
        android:layout_marginStart="168dp"
        android:layout_marginTop="1dp"
        android:background="@drawable/container" />

    <TextView
        android:id="@+id/tv_sex_shoes"
        android:layout_width="192dp"
        android:layout_height="18dp"
        android:layout_below="@+id/container"
        android:layout_alignParentStart="true"
        android:text="@string/some_id"
        android:textAppearance="@style/sex_shoes"
        android:lineSpacingExtra="4sp"
        android:translationY="-1.97sp"
        android:gravity="top" />

    <TextView
        android:id="@+id/tv_colors"
        android:layout_width="40dp"
        android:layout_height="17dp"
        android:layout_below="@+id/tv_sex_shoes"
        android:layout_alignParentStart="true"
        android:text="@string/colors"
        android:textAppearance="@style/colors"
        android:lineSpacingExtra="-2sp"
        android:gravity="center_vertical" />

    <View
        android:id="@+id/iv_colors"
        android:layout_width="45dp"
        android:layout_height="20dp"
        android:layout_below="@+id/tv_sex_shoes"
        android:layout_marginStart="114dp"
        android:layout_marginTop="83dp"
        android:layout_toEndOf="@id/tv_colors"
        android:background="@drawable/container_colors" />

    <TextView
        android:id="@+id/tv_size"
        android:layout_width="58dp"
        android:layout_height="17dp"
        android:layout_below="@+id/tv_sex_shoes"
        android:layout_toEndOf="@id/iv_colors"
        android:text="@string/size"
        android:textAppearance="@style/size"
        android:lineSpacingExtra="-2sp"
        android:gravity="center_vertical" />

    <View
        android:id="@+id/vector"
        android:layout_width="15.84dp"
        android:layout_height="7.1dp"
        android:layout_below="@+id/tv_sex_shoes"
        android:layout_toEndOf="@id/tv_size"
        android:background="@drawable/vector" />

    <View
        android:id="@+id/line_19"
        android:layout_width="312dp"
        android:layout_height="0dp"
        android:layout_below="@+id/tv_colors"
        android:layout_alignParentStart="true"
        android:background="@drawable/line_19" />

    <TextView
        android:id="@+id/tv_quantity"
        android:layout_width="54dp"
        android:layout_height="17dp"
        android:layout_below="@+id/line_19"
        />
</RelativeLayout>